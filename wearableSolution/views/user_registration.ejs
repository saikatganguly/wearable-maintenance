<html>
	<head>
	<title>Registration</title>
	<link href="/stylesheets/bootstrap.css" rel="stylesheet">
	<link href="/stylesheets/index.css" rel="stylesheet"> 
	<link href="/stylesheets/login.css" rel="stylesheet">
	<script type="text/javascript" src="/javascripts/jquery-2.1.1.js"></script>
	<script type="text/javascript" src="/javascripts/userregistration.js"></script>
	</head>
	<body style="background:rgb(254,249,245);">
		<!-- start of header -->
		<div >
			<div class="container-fluid" style="border-bottom: 5px solid #0A91C8;background:white;">
				<div class="row">
					<div class="col-md-12">
						<div class="container">
							<div class="row">
								<div class="col-xs-1 col-sm-1 col-md-1 col-lg-1 "  style="margin-bottom:3px;height:12%">
												
								</div>
															
							</div>
						</div>
					</div>
				</div>
				
			</div>
		</div>
		<!-- end of header -->
		
		<!-- start of container -->
		<div >  
			<div class="container-fluid" style="background:rgb(254,249,245);height:87%">
				<div class="row">
					<div class="col-md-12">
						<div class="container">
							<div style="margin-left:-1%">
								<div class="row" style="margin-top:1px;margin-bottom:5px">
									<div class="col-md-offset-2 col-md-8">
										<div style="margin-top:6%;margin-left:9%">
											<span style="font-size:24px;color:#29A6CF">Registration</span>
											<hr style="border-color:rgb(20,119,160) -moz-use-text-color -moz-use-text-color ">
										</div>
									</div>
								</div>
								<form action="/user/register"  onsubmit="return validate()"  method="post">
									<div class="row" style="margin-top:5%;margin-bottom:2px">
										<div class="col-md-offset-5 col-md-3">
											<input id="firstName" name="name" class="textlogin" type="text" placeholder="First Name" tabindex="1" onfocus="removeMessage('errFirstName');" autocomplete="off"/>
											<label style="color:red;" id="errFirstName"></label>
										</div>
									</div>
									
									<div class="row" style="margin-top:1%;margin-bottom:2px">
										<div class="col-md-offset-5 col-md-3">
											<input id="lastName" name="username" class="textlogin" type="text" placeholder="Last Name" tabindex="2" onfocus="removeMessage('errLastName');" autocomplete="off"/>
											<label style="color:red;" id="errLastName"></label>
										</div>
									</div>
									
									<div class="row" style="margin-top:1%;margin-bottom:2px">
										<div class="col-md-offset-5 col-md-3">
											<input id="emailid" name="email" class="textlogin" type="text" placeholder="Email id" tabindex="3" onfocus="removeMessage('errEmail');" autocomplete="off"/>
											<label style="color:red;" id="errEmail"></label>
										</div>
										
									</div>
									
									<div class="row" style="margin-top:1%;margin-bottom:2px">
										<div class="col-md-offset-5 col-md-3">
											<input class="textlogin" id="password" name="password" type="password" placeholder="Password" tabindex="4" onfocus="removeMessage('errPassword');" autocomplete="off"/>
											<label style="color:red;" id="errPassword"></label>
										</div>
									</div>
									
									<div class="row" style="margin-top:1%;margin-bottom:2px">
										<div class="col-md-offset-5 col-md-3">
											<input class="textlogin" type="password" id="retypepassword" placeholder="Confirm Password" tabindex="5" />
											<label style="color:red;" id="errRetypePassword"></label>
										</div>
									</div>
									
									<div class="row" style="margin-top:1%;margin-bottom:5px">
										<div class="col-md-offset-5 col-md-3">
											<button id="submit" tabindex="6" class="btn btn-second loginbutton"  type="submit">
										<b> Submit</b></button>
										</div>
									</div>
								</form>
							</div>
							
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- end of container -->
		
		<!-- start of footer 
		<div style="height:10%;">
		
		</div> 
		 end of footer -->
	</body>
</html>

